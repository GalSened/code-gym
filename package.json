    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.3",
    "next-auth": "^5.0.0-beta.30",
    "prisma": "^7.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
